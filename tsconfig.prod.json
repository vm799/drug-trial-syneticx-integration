{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmitOnError": false,
    "skipLibCheck": true,
    "strict": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "exclude": [
    "src/agents/*.ts",
    "src/composables/useApi.ts",
    "src/services/agentService.ts",
    "tests/**/*"
  ]
}