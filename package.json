{
  "name": "fusion-vue-tailwind-starter",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "server:install": "cd server && npm install",
    "server:dev": "cd server && npm run dev",
    "server:start": "cd server && npm start",
    "dev:fullstack": "concurrently \"npm run dev\" \"npm run server:dev\"",
    "setup": "npm install && npm run server:install",
    "start:production": "npm run build && concurrently \"npm run preview\" \"npm run server:start\"",
    "start": "node server.js"
  },
  "dependencies": {
